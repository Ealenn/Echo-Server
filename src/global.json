{
    "port": 80,
    "logs": {
        "ignore": {
            "ping": false
        }
    },
    "enable": {
        "host": true,
        "http": true,
        "request": true,
        "environment": true,
        "file": true
    },
    "commands": {
        "httpBody": {
            "query": "echo_body",
            "header": "x-echo-body"
        },
        "httpEnvBody": {
            "query": "echo_env_body",
            "header": "x-echo-env-body"
        },
        "httpCode": {
            "query": "echo_code",
            "header": "x-echo-code",
            "headerResponse": true
        },
        "time": {
            "query": "echo_time",
            "header": "x-echo-time"
        },
        "file": {
            "query": "echo_file",
            "header": "x-echo-file"
        }
    }
}